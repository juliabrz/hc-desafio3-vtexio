  "tab-list.item#home4": {
    "props": {
      "tabId": "palmeiras",
      "label": "Palmeiras"
    }
  },
  "tab-list.item#home5": {
    "props": {
      "tabId": "signos",
      "label": "Signos"
    }
  },
  "tab-list.item#home6": {
    "props": {
      "tabId": "guerreiros",
      "label": "Guerreiros"
    }
  },
  "tab-list.item#home7": {
    "props": {
      "tabId": "divertidos",
      "label": "Divertidos"
    }
  },
  "tab-content.item#home1": {
    "children": [
      "rich-text#home1",
      "search-result-layout.customQuery#promocoes"
    ],
    "props": {
      "tabId": "promocoes"
    }
  },
  "tab-content.item#home2": {
    "children": [
      "rich-text#home2"
