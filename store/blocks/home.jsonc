  "tab-content.item#home3": {
    "children": [
      "rich-text#home2"
    ],
    "props": {
      "tabId": "corintians"
    }
  },
  "tab-content.item#home4": {
    "children": [
      "rich-text#home2"
    ],
    "props": {
      "tabId": "palmeiras"
    }
  },
  "tab-content.item#home5": {
    "children": [
      "rich-text#home2"
    ],
    "props": {
      "tabId": "signos"
    }
  },
  "tab-content.item#home6": {
    "children": [
      "rich-text#home2"
    ],
    "props": {
      "tabId": "guerreiros"
    }
  },
  "tab-content.item#home7": {
    "children": [
      "rich-text#home2"
    ],
    "props": {
