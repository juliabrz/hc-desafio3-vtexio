{
  "store.home": { 
    "blocks": [
      "flex-layout.row#principal"
    ]
  },
  "flex-layout.row#principal":{
    "props":{
      "horizontalAlign": "center"
    },
    "children": [
      "flex-layout.col#all"
    ]
  },
  "flex-layout.col#all":{
    "props":{
      "blockClass": "flex-col-all",
      "width":"85%"
    },
    "children": [
      "tab-layout#home"
    ]
  },
  "tab-layout#home":{
    "children":[
      "tab-list#home",
      "list-context.image-list#demo",
      "tab-content#home"
    ]
  },
  "tab-list#home":{
    "children":[
      "tab-list.item#home1",
      "tab-list.item#home2",
      "tab-list.item#home3",
      "tab-list.item#home4",
      "tab-list.item#home5",
      "tab-list.item#home6",
      "tab-list.item#home7"
    ]
  },
  "tab-content#home": {
    "children": [
      "tab-content.item#home1",
      "tab-content.item#home2",
      "tab-content.item#home3",
      "tab-content.item#home4",
      "tab-content.item#home5",
      "tab-content.item#home6",
      "tab-content.item#home7"
      ]
  },
  "tab-list.item#home1": {
    "props": {
      "tabId": "promocoes",
      "label": "Promoções",
      "defaultActiveTab": true
    }
  },
  "tab-list.item#home2": {
    "props": {
      "tabId": "maravilhas",
      "label": "7 Maravilhas"
    }
  },
  "tab-list.item#home3": {
    "props": {
      "tabId": "corintians",
      "label": "Corinthians"
    }
